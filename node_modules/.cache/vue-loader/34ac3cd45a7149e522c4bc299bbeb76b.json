/crud-app-backend/client/src/components/TeamComponent.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"teams\">\n      <button type=\"button\" class=\"btn btn-primary\">\n          <i class=\"fa fa-user-plus\"></i>Add New Team</button>\n      <h3>{{viewTitle}}</h3>\n    <table>\n    <tr>\n      <th>text</th>\n      <th>StartDate</th>\n      <th>EndDate</th>\n      <th>Edit</th>\n      <th>Delete</th>\n    </tr>\n    <tr v-for=\"team in teams\" :key=\"team._id\">\n      <td>{{team.name}}</td>\n      <!-- <td><input type=\"text\" :readonly=\"shouldDisable\" @keyup.enter=\"getAllTeams\" ref=\"textField\" v-model=\"team.text\">{{team.text}}</td> -->\n      <td>{{team.startDate}}</td>\n      <td>{{team.endDate}}</td>\n      <td><i class=\"fa fa-pencil\"  @click=\"editTeam(team._id, team.text)\"></i></td>\n      <td><i class=\"fa fa-trash\" @click=\"deleteTeam(team._id)\"></i></td>\n    </tr>\n \n <!-- <input type=\"text\" v-model=\"text\" /><button @click=\"insertPost(text)\">add</button> -->\n\n</table>\n  </div>\n</template>\n\n<script>\nimport crudAxios from '../core/crudAxios'\nexport default {\n  name: 'TeamComponent',\n  props: {\n  },\n    data(){\n      return{\n          teams: [],\n          viewTitle: 'Team Management',\n          name: '',\n          startDate: '',\n          endDate: '',\n         \n      }\n  },\n\n  methods:{\n    //return teams\n    getAllTeams(){\n      crudAxios.get(`http://localhost:5000/api/teams`).then(data =>{\n        this.teams = data.data\n        console.log('data', data.data);\n      })\n    },\n    //add new team\n    insertTeam(name, startDate, endDate){\n      crudAxios.post(`http://localhost:5000/api/teams`, { \n          name: name,\n          startDate: startDate,\n          endDate: endDate\n          })\n      .then(this.name = name)\n      .then(this.startDate = startDate)\n      .then(this.endDate = endDate)\n    //   .finally(this.getAllTeams())\n    },\n\n    //delete team\n    deleteTeam(id){\n      crudAxios.delete(`http://localhost:5000/api/teams/${id}`)\n      .finally(this.getAllTeams())\n      \n    },\n    //update team\n      editTeam(id, text){\n        // text = this.$refs.textField.value\n        crudAxios.post(`http://localhost:5000/api/teams/${id}`,\n        { text: text,\n        updatedAt: new Date()})\n        .then(this.text = text)\n        .then(this.shouldDisable = false)\n         .finally(this.getAllTeams())\n        console.log('el',text)\n         \n      }\n  },\n  created(){\n    this.getAllTeams();\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.teams{\n    background-color: #0dcaf0;\n    padding: 20px;\n}\nfa{\n    cur\n}\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\ntable {\n  font-family: arial, sans-serif;\n  border-collapse: collapse;\n  width: 100%;\n}\n\ntd, th {\n  /* border: 1px solid #dddddd; */\n  text-align: left;\n  padding: 8px;\n}\n\ntr:nth-child(even) {\n  background-color: #dddddd;\n}\n</style>\n"]}]}