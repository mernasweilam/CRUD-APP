@click=\"insertPost(text)\">add</button> -->\n\n</table>\n\n  </div>\n</template>\n\n<script>\nimport crudAxios from '../core/crudAxios'\n// import Datepicker from '../../node_modules/vuejs-datepicker'; \nexport default {\n  name: 'TeamComponent',\n  props: {\n  },\n  components: {\n    // Datepicker\n  },\n    data(){\n      return{\n          teams: [],\n          viewTitle: 'Team Management',\n          name: '',\n          startDate: '',\n          endDate: '',\n          addBtnClicked: false\n         \n      }\n  },\n\n  methods:{\n    //return teams\n    getAllTeams(){\n      crudAxios.get(`http://localhost:5000/api/teams`).then(data =>{\n        this.teams = data.data\n        console.log('data', data.data);\n      })\n    },\n    //add new team\n    insertTeam(name, startDate, endDate){\n      crudAxios.post(`http://localhost:5000/api/teams`, { \n          name: name,\n          startDate: startDate,\n          endDate: endDate\n          })\n      .then(this.name = name)\n      .then(this.startDate = startDate)\n      .then(this.endDate = endDate)\n    //   .finally(this.getAllTeams())\n    },\n\n    //delete team\n    deleteTeam(id){\n      crudAxios.delete(`http://localhost:5000/api/teams/${id}`)\n      .finally(this.getAllTeams())\n      \n    },\n    //update team\n      editTeam(id, text){\n        // text = this.$refs.textField.value\n        crudAxios.post(`http://localhost:5000/api/teams/${id}`,\n        { text: text,\n        updatedAt: new Date()})\n        .then(this.text = text)\n        .then(this.shouldDisable = false)\n         .finally(this.getAllTeams())\n        console.log('el',text)\n         \n      }\n  },\n  created(){\n    this.getAllTeams();\n  },\n  mounted(){\n      console.log('addBtnClicked',this.addBtnClicked)\n  },\n  updated() {\n      console.log('addBtnClicked',this.addBtnClicked)\n  },\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.teams{\n    background-color: #0dcaf0;\n    padding: 20px;\n}\n.fa{\n    cursor: pointer;\n}\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\ntable {\n  font-family: arial, sans-serif;\n  border-collapse: collapse;\n  width: 100%;\n}\n\ntd, th {\n  /* border: 1px solid #dddddd; */\n  text-align: left;\n  padding: 8px;\n}\n\ntr:nth-child(even) {\n  background-color: #dddddd;\n}\n</style>\n"],"sourceRoot":""}]}